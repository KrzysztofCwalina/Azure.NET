{
  "title": "Contact",
  "type": "object",
  "required": [ "First", "Last" ],
  "properties": {
    "First": {
      "type": "string"
    },
    "Last": {
      "type": "string"
    },
    "Age": {
      "type": "number",
      "minimum": 0
    },
    "Phones": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "Address": {
      "type": "object",
      "properties": {
        "Zip": {
          "type": "number"
        },
        "City": {
          "type": "string"
        }
      }
    }
  }
}